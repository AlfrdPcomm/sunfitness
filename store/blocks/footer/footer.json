    "infinite": true,
    "showNavigationArrows": "never",
    "showPaginationDots": "never"
  },
  "children": ["image#1", "image#2", "image#3","image#4"]
},

"image#1": {
  "props": {
    "src": "https://sunfitnessmx.myvtex.com/arquivos/facebook.png",
